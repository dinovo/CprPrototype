<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CprPrototype.View.ExtraDrugs"
             Title="Medicin">
    <ContentPage.Content>
        <StackLayout
            Spacing="10">
            <Button x:Name="btnBikarbonat"
                Text="Bikarbonat"
                Margin="10, 0, 10, 0"
                VerticalOptions="FillAndExpand"
                Clicked="btnBikarbonat_Clicked" />
            <Button x:Name="btnCalcium"
                Text="Calcium"
                Margin="10, 0, 10, 0"
                VerticalOptions="FillAndExpand"
                Clicked="btnCalcium_Clicked" />
            <Button x:Name="btnMagnesium"
                Text="Magnesium"
                Margin="10, 0, 10, 0"
                VerticalOptions="FillAndExpand"
                Clicked="btnMagnesium_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>